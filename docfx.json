{
    "build": {
        "content": [
            {
                "files": [
                    "sharepoint/**.md",
                    "sharepoint/**/toc.yml",
                    "collabware/**.md",
                    "collabware/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": ["images/**"]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": ["default"],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}
